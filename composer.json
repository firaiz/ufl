  "version": "5.0.20",
  "authors": [
    { "name": "romer", "email": "romer.t.co@gmail.com" },
    { "name":  "Kenichi Yoshida", "email": "k.yoshida@miraizu.jp"}
  ],
  "require": {
    "php": ">=8.1",
    "smarty/smarty": "v4.3.0",
    "doctrine/inflector": "2.*",
    "doctrine/cache": "1.*",
    "doctrine/dbal" : "4.0.0-beta1",
    "ext-pdo": "*",
    "ext-json": "*" ,
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": { "Firaiz\\Ufl\\" : "src", "Firaiz\\Ufl\\TestClass\\" : "test/class", "Firaiz\\Ufl\\TestCase\\" : "test/case" },
    "files" : [
      "src/helpers.php"
    ]
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "9.5.26",
    "rector/rector": "^0.15.0"
  }
}
